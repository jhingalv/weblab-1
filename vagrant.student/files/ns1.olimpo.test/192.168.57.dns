; Reverse zone

$ORIGIN 57.168.192.in-addr.arpa.
$TTL 604800

@   IN SOA ns1.olimpo.test. root.olimpo.test. (
        2025113001  ; Serial
        604800      ; Refresh
        86400       ; Retry
        2419200     ; Expire
        86400       ; Minimum TTL
)

@   IN NS ns1.olimpo.test.

10  IN PTR ns1.olimpo.test.
11  IN PTR mercurio.olimpo.test.
