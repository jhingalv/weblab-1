; olimpo.test domain direct zone

$ORIGIN olimpo.test.
$TTL 86400

@   IN SOA ns1.olimpo.test. root.olimpo.test. (
        2025113001  ; Serial
        604800      ; Refresh
        86400       ; Retry
        2419200     ; Expire
        86400       ; Minimum TTL
)

; DNS server
@       IN NS   ns1.olimpo.test.

; A registers
ns1        IN A   192.168.57.10
mercurio   IN A   192.168.57.11

; Aliases (CNAME)
apolo      IN CNAME mercurio.olimpo.test.
atenea     IN CNAME mercurio.olimpo.test.
